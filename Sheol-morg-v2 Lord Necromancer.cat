            <characteristic name="WS" typeId="716f-8261-c374-f462">2</characteristic>
            <characteristic name="S" typeId="65de-0c6b-9559-e9d4">5</characteristic>
            <characteristic name="T" typeId="c9bb-0a5f-7e53-c580">6</characteristic>
