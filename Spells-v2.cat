    <selectionEntry type="upgrade" import="true" name="Fireball" hidden="false" id="1fc5-d2a0-ce06-854c" publicationId="30f0-6ecb-d1bf-eb3e" page="70">
      <costs>
        <cost name="Points" typeId="78f2-651d-2f2e-98bf" value="7"/>
