    <selectionEntry type="upgrade" import="true" name="Blizzard" hidden="false" id="63bf-58ae-75f0-e27b" publicationId="30f0-6ecb-d1bf-eb3e" page="70">
      <costs>
        <cost name="Points" typeId="78f2-651d-2f2e-98bf" value="6"/>
        <cost name="Bases" typeId="4f83-a514-6b6a-5475" value="0"/>
      </costs>
      <profiles>
        <profile name="Blizzard" typeId="c24f-eb97-d49f-12c9" typeName="Spell" hidden="false" id="7ad4-4d45-c81e-dad3">
          <characteristics>
            <characteristic name="Description" typeId="fada-0619-6d75-fefe">Choose target enemy unit within 30 cm that is not engaged in combat. Roll a D6 – a score of 4 or less means a successful spell. Move the unit 5 cm forwards or backwards in a straight line.</characteristic>
            <characteristic name="Test" typeId="bf6f-5cc5-80ba-b400">4-</characteristic>
          </characteristics>
        </profile>
      </profiles>
      <constraints>
        <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="bc68-436b-1f08-72c9" includeChildSelections="false"/>
