    <selectionEntry type="upgrade" import="true" name="Thunderbolt" hidden="false" id="add1-4600-40d1-283f" publicationId="30f0-6ecb-d1bf-eb3e" page="71">
      <costs>
        <cost name="Points" typeId="78f2-651d-2f2e-98bf" value="7"/>
        <cost name="Bases" typeId="4f83-a514-6b6a-5475" value="0"/>
      </costs>
      <profiles>
        <profile name="Thunderbolt" typeId="c24f-eb97-d49f-12c9" typeName="Spell" hidden="false" id="1405-4131-7e74-7bdc" page="71" publicationId="30f0-6ecb-d1bf-eb3e">
          <characteristics>
            <characteristic name="Description" typeId="fada-0619-6d75-fefe">Choose target enemy unit on the battlefield that is not engaged in combat. Roll a D6 – a score of 2 or less means a successful spell. The chosen unit is dealt 3 Wounds. If the unit has any save, or it is on a base bigger than 20 mm, it is dealt 4 Wounds instead. No saves are permitted against Thunderbolt.</characteristic>
            <characteristic name="Test" typeId="bf6f-5cc5-80ba-b400">2-</characteristic>
          </characteristics>
        </profile>
      </profiles>
      <constraints>
        <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="8702-81e7-60fd-2f6f" includeChildSelections="false"/>
      </constraints>
