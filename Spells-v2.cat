      </constraints>
    </selectionEntry>
    <selectionEntry type="upgrade" import="true" name="Whispers of the Gods" hidden="false" id="d617-afa5-f9aa-ec60" publicationId="30f0-6ecb-d1bf-eb3e" page="70">
      <costs>
        <cost name="Points" typeId="78f2-651d-2f2e-98bf" value="3"/>
        <cost name="Bases" typeId="4f83-a514-6b6a-5475" value="0"/>
      </costs>
      <profiles>
        <profile name="Whispers of the Gods" typeId="c24f-eb97-d49f-12c9" typeName="Spell" hidden="false" id="9bb4-2b48-94bc-646f" page="70" publicationId="30f0-6ecb-d1bf-eb3e">
          <characteristics>
            <characteristic name="Description" typeId="fada-0619-6d75-fefe">Choose target General within 30 cm. Roll a D6 – a score of 4 or less means a successful spell. Choose a +2 or -2 modifier. The selected General will receive it in the next Initiative Test.</characteristic>
            <characteristic name="Test" typeId="bf6f-5cc5-80ba-b400">4-</characteristic>
          </characteristics>
