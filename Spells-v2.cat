    <selectionEntry type="upgrade" import="true" name="Death Blast" hidden="false" id="61b4-1a3b-f6cf-f552" publicationId="30f0-6ecb-d1bf-eb3e" page="71">
      <costs>
        <cost name="Points" typeId="78f2-651d-2f2e-98bf" value="4"/>
        <cost name="Bases" typeId="4f83-a514-6b6a-5475" value="0"/>
      </costs>
      <profiles>
        <profile name="Death Blast" typeId="c24f-eb97-d49f-12c9" typeName="Spell" hidden="false" id="dedd-c130-6c3e-e250" publicationId="30f0-6ecb-d1bf-eb3e" page="71">
          <characteristics>
            <characteristic name="Description" typeId="fada-0619-6d75-fefe">Choose up to three target enemy units (but not single models) within 40 cm with wounded bases. Roll a D6. On a score of 4 or less, the chosen units must remove the wounded bases.</characteristic>
            <characteristic name="Test" typeId="bf6f-5cc5-80ba-b400">4-</characteristic>
          </characteristics>
        </profile>
      </profiles>
