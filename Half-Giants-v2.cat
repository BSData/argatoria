        <modifier type="set" value="-1" field="f618-14a2-1b7b-a22d">
          <conditions>
            <condition type="atLeast" value="1" field="selections" scope="roster" childId="d7cd-945c-8173-b79d" shared="true" includeChildSelections="true" includeChildForces="true"/>
