    <selectionEntry type="model" import="true" name="Pakun" hidden="false" id="5fba-eb86-faf5-1561" publicationId="30f0-6ecb-d1bf-eb3e" page="148">
      <costs>
        <cost name="Points" typeId="78f2-651d-2f2e-98bf" value="42"/>
        <cost name="Bases" typeId="4f83-a514-6b6a-5475" value="1"/>
      </costs>
      <categoryLinks>
        <categoryLink name="Hero" hidden="false" id="169c-0151-7f43-12b9" targetId="0b0b-5bbc-9d5e-9b0c" primary="false"/>
        <categoryLink name="Legendary Hero" hidden="false" id="83c2-52c3-0f6b-dc13" targetId="b8ea-5e96-acf3-4bca" primary="true"/>
      </categoryLinks>
      <rules>
        <rule name="Pakun" id="3728-70e8-b2a2-6adc" hidden="false">
          <description>Pakun can be fielded in every army that may deploy the units of Half-Giants, regardless of the number of other heroes. Pakun has the special items and abilities listed below.</description>
        </rule>
        <rule name="Crusher" id="b310-c39b-1763-ed73" hidden="false" publicationId="30f0-6ecb-d1bf-eb3e" page="148">
          <description>Thanks to this mysterious artefact hammer, Pakun grants the unit he is attached to additional D6+6 attacks. In addition, the unit Pakun is attached to changes its Strength (S) to 4 if it has lesser.</description>
        </rule>
        <rule name="Oklar" id="4f95-4d86-2850-be01" hidden="false" publicationId="30f0-6ecb-d1bf-eb3e" page="148">
          <description>An enemy unit attacking a unit that Pakun is attached to must subtract 3 dice from the pool.</description>
        </rule>
        <rule name="Dark Humour" id="fe3b-2111-402a-03ef" hidden="false" publicationId="30f0-6ecb-d1bf-eb3e" page="148">
          <description>If Pakun is attached to a friendly Half-Giants unit, the unit may add or subtract 1 from its LD during any test.</description>
        </rule>
      </rules>
      <constraints>
        <constraint type="max" value="1" field="selections" scope="roster" shared="true" id="de99-13f2-fc9c-f82e" includeChildSelections="true" includeChildForces="true"/>
      </constraints>
    </selectionEntry>
